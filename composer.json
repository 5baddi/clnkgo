{
    "name": "baddiservices/social-rocket",
    "type": "project",
    "description": "Easy tools to shape your affiliate program.",
    "keywords": ["tool", "shopify", "affiliate"],
    "authors": [
        {
            "name": "BADDI Services",
            "email": "services@baddi.info",
            "homepage": "https://baddi.info"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fakerphp/faker": "^1.14",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "imliam/laravel-env-set-command": "^1.3",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "themsaid/wink": "^1.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BADDIServices\\SocialRocket\\": "app/",
            "BADDIServices\\SocialRocket\\Blog\\": "Blog/",
            "BADDIServices\\SocialRocket\\Documentation\\": "Documentation/",
            "BADDIServices\\SocialRocket\\Common\\": "Common/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan cache:clear",
            "chmod -R 777 public/",
            "chmod -R 755 bootstrap\\/cache\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
